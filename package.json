  "dependencies": {
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.3.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^1.3.5",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-table": "^7.0.28",
    "axios": "^0.21.1",
    "env-cmd": "^10.1.0",
    "framer-motion": "^4.1.17",
    "i18next": "^19.9.0",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.1.1",
    "joi": "^17.4.0",
    "oidc-client": "^1.11.5",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-i18next": "^11.8.8",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-table": "7.6.3",
    "redux-logger": "^3.0.6",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.1.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-logger": "^3.0.8",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "env-cmd -f .env.local npm run start",
    "start:dev:local": "yarn start:dev",
    "start:dev:local:secure": "set HTTPS=true&&yarn start:dev",
    "build": "react-scripts build",
    "build:prod": "env-cmd -f .env npm run build",
    "build:dev": "env-cmd -f .env.development npm run build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.+(ts|tsx|js|jsx)\"",
    "lint": "eslint \"src/**/*.+(ts|tsx|js|jsx)\"",
    "lint:fix": "yarn lint --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": "warn",
      "newline-before-return": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
